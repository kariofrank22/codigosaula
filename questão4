#include <stdio.h>

void divisao(int num1,int num2){

    int result;

    result=num1%num2;

    printf("O resto da divisão e:%d",result);

    return 0;

}

int main(){

    int num1,num2;

    printf("Entre com o 1º numero:");
    scanf("%d",&num1);

    printf("Entre com o 2º numero:");
    scanf("%d",&num2);

    divisao(num1,num2);

    return 0;

}
